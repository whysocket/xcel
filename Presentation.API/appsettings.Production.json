{
  "Environment": "Production",
  "Api": {
    "Webhooks": {
      "DiscordUrl": "${DISCORD_WEBHOOK_URL}"
    },
    "Cors": {
      "FrontendUrl": "https://chat.whysocket.dev"
    }
  },
  "Infra": {
    "Database": {
      "ConnectionString": "${DB_CONNECTION_STRING}",
      "DevPowers": {
        "Recreate": "None",
        "Migrate": "None"
      }
    },
    "Email": {
      "Host": "smtp.gmail.com",
      "Port": 587,
      "Username": "Support",
      "Password": "${EMAIL_PASSWORD}",
      "FromAddress": "eduardo@xceltutors.com",
      "EnableSsl": true
    },
    "Auth": {
      "Jwt": {
        "Issuer": "local-xcel",
        "Audience": "front-xcel",
        "SecretKey": "${JWT_SECRET_KEY}",
        "ExpireInMinutes": 1440
      }
    }
  }
}